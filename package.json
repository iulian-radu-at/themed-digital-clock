{
  "name": "themed-digital-clock",
  "version": "1.0.1",
  "description": "A digital clock with a dark and a light theme",
  "main": "lib/index.js",
  "sideEffects": false,
  "scripts": {
    "build": "rimraf ./lib && tsc",
    "deploy": "cross-env NODE_ENV=production npm run build && npm publish",
    "test": "echo \"Error: no test specified\" && exit 1",
    "storybook": "start-storybook -c storybook",
    "build-storybook": "build-storybook -c storybook -o docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iulian-radu-at/themed-digital-clock.git"
  },
  "files": [
    "lib",
    "index.d.ts"
  ],
  "keywords": [
    "clock",
    "digital clock",
    "react",
    "react-component",
    "ui"
  ],
  "author": "Iulian Radu <iulian.radu@gmx.at>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iulian-radu-at/themed-digital-clock/issues"
  },
  "homepage": "https://github.com/iulian-radu-at/themed-digital-clock#readme",
  "dependencies": {
    "digital-digit": "^1.0.2",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.26",
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@storybook/react": "^5.1.11",
    "@types/babel-core": "^6.25.6",
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/moment": "^2.13.0",
    "@types/moment-timezone": "^0.5.12",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/rimraf": "^2.0.2",
    "@types/storybook__react": "^4.0.2",
    "babel-core": "^6.26.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "cross-env": "^5.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "rimraf": "^3.0.0",
    "typescript": "^3.5.3"
  },
  "peerDependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "./node_modules",
      "./history"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "testRegex": "src/*(/(__tests?__/)([^_].*/)*?[^_][^/]*?\\.(test|spec)?\\.(js?))$",
    "setupFiles": [
      "./enzymeAdapter.setup.js"
    ]
  }
}
